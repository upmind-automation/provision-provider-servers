{
    "name": "upmind/provision-provider-servers",
    "description": "This provision category contains common functions used in provisioning flows for servers on various popular cloud platforms",
    "type": "library",
    "require": {
        "php": "8.1 - 8.3",
        "upmind/provision-provider-base": "^4.0",
        "upmind/linode-api": "dev-v2-dev || ^1.2",
        "ext-dom": "*",
        "ext-simplexml": "*"
    },
    "license": "GPL-3.0",
    "autoload": {
        "psr-4": {
            "Upmind\\ProvisionProviders\\Servers\\": "src/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10"
    },
    "authors": [
        {
            "name": "Harry Lewis",
            "email": "harry@upmind.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Upmind\\ProvisionProviders\\Servers\\LaravelServiceProvider"
            ]
        }
    }
}
